<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ProjectBlue"
    android:orientation="vertical">

    <TextView
        android:id="@+id/dmsDetStartDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:textColor="@color/WhiteSmoke"
        android:layout_below="@+id/dmsDetQuestioner"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="115dp" />

    <TextView
        android:id="@+id/dmsDetEndDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:textColor="@color/WhiteSmoke"
        android:layout_above="@+id/dmsDetExtra"
        android:layout_toEndOf="@+id/dmsDetWinner"
        android:layout_marginStart="38dp" />

    <TextView
        android:id="@+id/dmsDetQuestion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:padding="5dp"
        android:text="Titel"
        android:textColor="@color/WhiteSmoke"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/dmsDetExtra"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/dmsDetStartDate"
        android:layout_marginTop="31dp"
        android:background="@drawable/textviewborder"
        android:paddingBottom="3dp"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:text="New Text"
        android:textColor="@color/WhiteSmoke" />

    <TextView
        android:id="@+id/dmsDetWinner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="48dp"
        android:text="New Text"

        android:textColor="@color/WhiteSmoke" />

    <TextView
        android:id="@+id/dmsDetQuestioner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"

        android:layout_below="@+id/dmsDetQuestion"
        android:paddingBottom="5dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="2dp"
        android:text="OP"
        android:textColor="@color/WhiteSmoke"
        android:textStyle="italic" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/imageView2"
        android:layout_alignTop="@+id/dmsDetEndDate"
        android:layout_alignParentStart="true"
        android:src="@drawable/calendaricon"
        android:background="@color/ProjectBlue"/>

</RelativeLayout>